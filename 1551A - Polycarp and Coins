#include <iostream>
#include<math.h>
using namespace std;

int main()
{
    int t;
    cin>>t;
    while(t--)
    {
      double n;
      cin>>n;
      int c2 = 0;
      int mod = int(n) % 3;
      if(mod == 1)
       {
         c2 = int((n-1)/3);
       }
       else
       {
         c2 = ceil(n/3);
       }
       int c1 = n - (2*c2);
       cout<<c1<<" "<<c2<<endl;
       
      }
}
