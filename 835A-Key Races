#include <iostream>
#include <string>
#include <algorithm>
#include <map>
#include <cmath>
#include <set>
using namespace std;

#define ll long long
int main()
{
	int s, v1, v2, t1, t2;
	cin >> s >> v1 >> v2 >> t1 >> t2;
	int p1 = (2 * t1) + (s * v1);
	int p2 = (2 * t2) + (s * v2);
	if (p1 < p2)
		cout << "First";
	else if (p1 > p2)
		cout << "Second";
	else
		cout << "Friendship";

}
