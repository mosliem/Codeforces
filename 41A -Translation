#include <iostream>
#include <algorithm>
#include <math.h>
#include <string> 
using namespace std;
#define ll long long

int main()
{
	string s1, s2;
	cin >> s1 >> s2;
	for (int i = 0, j = s2.length() - 1; i < s1.length(); i++, j--)
	{
		if (s1[i] != s2[j])
		{
			cout << "NO";
			return 0;
		}
	}
	cout << "YES";
}

